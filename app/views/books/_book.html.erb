<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Date published:</strong>
    <%= book.date_published %>
  </p>

  <p>
    <strong>Publisher name:</strong>
    <%= book.publisher_name %>
  </p>

</div>
